local function B(Text)
local G = Instance.new("TextButton", F)
G.Size = UDim2.new(0.95, 0, 0.2, 0)
G.BackgroundColor3 = Color3.new(1, 1, 1)
G.Text = Text
G.Name = Text
G.TextColor3 = Color3.new(0, 0, 0)
G.TextScaled = true
return G
end
B("Become Sbeve (44k slaps)")
B("Become Sbeve (Sbeve player)")
B("Sbeve all Players")
B("Invisible (Ghost)")

local function RequireSbeve()
	if game.Players.LocalPlayer.Character:FindFirstChild("stevebody") then 
		game:GetService("StarterGui"):SetCore("SendNotification",{Title = "Error", Text = "You already became Sbeve!", Duration = 1})
		return
	end
	if game.Players.LocalPlayer.leaderstats.Slaps.Value >= 44000 and game.Players.LocalPlayer.Character:FindFirstChild("entered") == nil then
		fireclickdetector(workspace.Lobby["Sbeve"].ClickDetector)
		game:GetService("ReplicatedStorage").GeneralAbility:FireServer()
		wait(2)
		fireclickdetector(workspace.Lobby["Ghost"].ClickDetector)
		for i = 1,2 do
			game.ReplicatedStorage.Ghostinvisibilityactivated:FireServer()
			game.ReplicatedStorage.Ghostinvisibilityactivated:FireServer()
		end
		fireclickdetector(workspace.Lobby["Default"].ClickDetector)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.workspace.Origo.CFrame * CFrame.new(0,-5,0)
	elseif game.Players.LocalPlayer.leaderstats.Glove.Value == "Sbeve" and game.Players.LocalPlayer.Character:FindFirstChild("entered") ~= nil then
		game:GetService("ReplicatedStorage").KrakenArm:FireServer()
	elseif game.Players.LocalPlayer.Character:FindFirstChild("entered") then
		game:GetService("StarterGui"):SetCore("SendNotification",{Title = "Error", Text = "Do not use while you entered portal!", Duration = 1})
	elseif game.Players.LocalPlayer.leaderstats.Slaps.Value <= 44000 then
		game:GetService("StarterGui"):SetCore("SendNotification",{Title = "Error", Text = "Not enough slaps!", Duration = 1})
	end
end

local function BecomeSbeve()
    if game.Players.LocalPlayer.Character:FindFirstChild("stevebody") then 
        game:GetService("StarterGui"):SetCore("SendNotification",{Title = "Error!", Text = "You already became Sbeve!", Duration = 1})
        return
    end

    local foundSbeve = false
    for _, v in ipairs(game.Players:GetPlayers()) do
        if v ~= game.Players.LocalPlayer and v.Character:FindFirstChild("stevebody") then
            foundSbeve = true
            game:GetService("StarterGui"):SetCore("SendNotification",{Title = "Notification!", Text = "Found a Sbeve, becoming Sbeve!", Duration = 1})
            OgPlace2 = v.Character:FindFirstChild("HumanoidRootPart").CFrame
            v.Character:FindFirstChild("HumanoidRootPart").CFrame = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame
            wait()
            v.Character:FindFirstChild("HumanoidRootPart").CFrame = OgPlace2
        end
    end

    if not foundSbeve then
        game:GetService("StarterGui"):SetCore("SendNotification",{Title = "Error", Text = "Cannot find Sbeve!", Duration = 1})
    end
end
local function teleportPlayers()
    if not game.Players.LocalPlayer.Character:FindFirstChild("stevebody") then 
        game:GetService("StarterGui"):SetCore("SendNotification",{Title = "Error!", Text = "You have to become Sbeve!", Duration = 1})
        return
    end
    if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        for _, player in ipairs(game.Players:GetPlayers()) do
            if player ~= game.Players.LocalPlayer then
                if player.Character:FindFirstChild("HumanoidRootPart") and player.Character.Head:FindFirstChild("UnoReverseCard") == nil and player.Character:FindFirstChild("entered") and player.Character:FindFirstChild("stevebody") == nil and player.Character.Ragdolled.Value == false then
	                OgPlace = player.Character:FindFirstChild("HumanoidRootPart").CFrame
                    player.Character:FindFirstChild("HumanoidRootPart").CFrame = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame
                    player.Character.HumanoidRootPart.CanCollide = true
                    wait()
                    player.Character:FindFirstChild("HumanoidRootPart").CFrame = OgPlace
                 elseif player.Character:FindFirstChild("stevebody") ~= nil then
	                player.Character.HumanoidRootPart.CanCollide = false
                end
            end
        end
    end
end

local StartLop = false
for _, Button in ipairs(F:GetChildren()) do
    if Button:IsA("TextButton") then
        Button.MouseButton1Click:Connect(function()
	        if Button.Name == "Become Sbeve (44k slaps)" then
	RequireSbeve()
	        elseif Button.Name == "Become Sbeve (Sbeve player)" then
	BecomeSbeve()
	        elseif Button.Name == "Sbeve all Players" then
				teleportPlayers()
	        elseif Button.Name == "Invisible (Ghost)" then
				if game.Players.LocalPlayer.Character:FindFirstChild("entered") == nil then
					fireclickdetector(workspace.Lobby["Ghost"].ClickDetector)
					for i = 1,2 do
						game.ReplicatedStorage.Ghostinvisibilityactivated:FireServer()
						game.ReplicatedStorage.Ghostinvisibilityactivated:FireServer()
					end
					fireclickdetector(workspace.Lobby["Default"].ClickDetector)
				else
					game:GetService("StarterGui"):SetCore("SendNotification",{Title = "Error", Text = "Do not use while you entered portal!", Duration = 1})
				end
	        end
        end)
    end
end
